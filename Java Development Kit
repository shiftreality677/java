public class ArraySum {
    public static void main(String[] args) {
        int[] array = {5, 10, 15, 20, 25}; // Замените значениями своего массива

        int sum = 0;
        for (int num : array) {
            sum += num;
        }

        System.out.println("Сумма элементов массива: " + sum);
    }
}
